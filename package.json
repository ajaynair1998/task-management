{
   "name": "task-management",
   "version": "1.0.0",
   "description": "Task management system",
   "main": "./dist/app.js",
   "scripts": {
      ".": "npm run dev && --watch src --watch libs ./src/app.ts",
      "start": "npm run serve",
      "serve": "node .",
      "format": "prettier --write \"src/**/*.ts\"",
      "dev": "ttsc -p tsconfig.build.json && nodemon",
      "start:debug": "nodemon --config nodemon-debug.json",
      "build": "ttsc -p .",
      "build:dev": "ttsc && pm2 start pm2.yaml",
      "build:prod": "ttsc && NODE_PORT=3005 pm2 start pm2.yaml -n task-management --env production",
      "build-ts": "ttsc",
      "watch-node": "nodemon --inspect=0.0.0.0:9229 .",
      "watch-ts": "tsc -w",
      "delete": "pm2 delete task-management-app",
      "stop": "pm2 stop task-management-app",
      "lint": "eslint \"src/**/*.ts\" --fix",
      "pretest": "lint-staged",
      "test": "NODE_ENV=testing jest --no-cache --runInBand  --colors --coverage --silent --passWithNoTests --detectOpenHandles  --forceExit",
      "test:e2e": "jest"
   },
   "lint-staged": {
      "*": [
         "npm run format",
         "npm run lint",
         "git add"
      ]
   },
   "husky": {
      "hooks": {
         "pre-push": "npm test"
      }
   },
   "keywords": [],
   "author": "Phat Vo",
   "license": "Phat Vo",
   "dependencies": {
      "bluebird": "^3.7.2",
      "class-validator": "^0.13.1",
      "class-validator-jsonschema": "^2.2.0",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "csurf": "^1.11.0",
      "dotenv": "^8.1.0",
      "eslint-config-prettier": "^6.15.0",
      "event-stream": "^4.0.1",
      "express": "^4.17.1",
      "express-basic-auth": "^1.2.0",
      "helmet": "^3.21.0",
      "ioredis": "^4.24.4",
      "mysql": "^2.18.1",
      "npm": "^6.14.10",
      "pg-connection-string": "^2.4.0",
      "pm2": "^4.4.1",
      "reflect-metadata": "^0.1.13",
      "routing-controllers": "^0.7.7",
      "routing-controllers-openapi": "^2.2.0",
      "swagger-ui-express": "^4.1.6",
      "ts-node": "^8.8.1",
      "typedi": "^0.8.0",
      "typeorm": "^0.2.31"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^4.12.0",
      "@types/bluebird": "^3.5.33",
      "@types/body-parser": "^1.17.1",
      "@types/compression": "^1.0.1",
      "@types/cors": "^2.8.6",
      "@types/csurf": "^1.11.0",
      "@types/dotenv": "^6.1.1",
      "@types/express": "^4.17.1",
      "@types/helmet": "0.0.44",
      "@types/multer": "^1.4.2",
      "@types/node": "^12.7.4",
      "@typescript-eslint/parser": "^4.12.0",
      "eslint": "^7.2.0",
      "eslint-config-airbnb": "^18.2.1",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-promise": "^4.2.1",
      "eslint-plugin-react": "^7.22.0",
      "eslint-plugin-react-hooks": "^4.0.0",
      "eslint-plugin-standard": "^4.1.0",
      "husky": "^4.3.7",
      "ioredis-mock": "^5.4.1",
      "jest": "^26.6.3",
      "lint-staged": "^10.5.3",
      "node-pre-gyp": "0.12.0",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "sqlite3": "^5.0.2",
      "standard": "^14.3.4",
      "supertest": "^6.0.1",
      "ts-jest": "^26.4.4",
      "tsconfig-paths": "^3.9.0",
      "ttypescript": "^1.5.12",
      "typescript": "^3.9.7",
      "typescript-transform-paths": "^2.0.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "coverage",
      "testEnvironment": "node"
   }
}
